{
  "name": "summitcare",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "preview": "vite preview",
    "typecheck:frontend": "tsc -p tsconfig.json --noEmit",
    "typecheck:server": "tsc -p tsconfig.server.json --noEmit",
    "typecheck:client": "tsc -p tsconfig.client.json --noEmit",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "sh -c 'MAJOR=$(node -p \"process.versions.node.split(\\\".\\\")[0]\"); if [ \"$MAJOR\" -lt 20 ]; then echo \"Skipping tests on Node<$MAJOR (require >=20)\"; exit 0; fi; vitest run --config ./vitest.config.ts --coverage --passWithNoTests'",
    "test:watch": "vitest --config ./vitest.config.ts --watch",
    "prepare": "husky",
    "check:routes": "bash scripts/find-duplicate-api-files.sh",
    "check:edge": "bash scripts/check-edge-runtime.sh",
    "fix:edge": "bash scripts/add-edge-runtime.sh",
    "check:cors:strict": "bash scripts/check-cors-block-strict.sh",
    "fix:cors:block": "node scripts/patch-cors-in-responses.mjs",
    "dev:api": "vercel dev",
    "dev:web": "vite",
    "dev:all": "concurrently -n api,web -c blue,green \"vercel dev\" \"vite\"",
    "test:api": "bash scripts/test-api.sh",
    "db:setup": "scripts/setup-local-db.sh",
    "db:setup:nosample": "IMPORT_SAMPLE=false scripts/setup-local-db.sh",
    "db:import:schema": "SCHEMA_FILE=schema/summitcare.sql scripts/setup-local-db.sh",
    "db:import:sample": "SCHEMA_FILE=/dev/null IMPORT_SAMPLE=true SAMPLE_FILE=schema/summitcare_sample_data.sql scripts/setup-local-db.sh",
    "push:quick": "git add -A && git commit -m \"chore: quick commit\" && git push",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "lint": "eslint . --ext .js,.mjs,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.mjs,.ts,.tsx --fix --max-warnings=0",
    "verify": "pnpm run format:check && pnpm run lint && pnpm run typecheck:server",
    "verify:quick": "pnpm run format:check && eslint . --quiet --cache --cache-location .eslintcache",
    "vercel:build": "vercel build",
    "vercel:dev": "vercel dev",
    "push:verified": "./scripts/verify-and-push.sh",
    "precommit:strict": "pnpm exec lint-staged && pnpm run verify",
    "db:check": "ts-node src/lib/checkDb.ts",
    "db:seed": "ts-node src/lib/seed.ts",
    "start": "next start -p 3000",
    "rename:jsx-preview": "bash scripts/rename-jsx-to-tsx.sh --preview",
    "rename:jsx-to-tsx": "bash scripts/rename-jsx-to-tsx.sh",
    "ts:nocheck-ui": "bash scripts/ts-nocheck-ui.sh",
    "ts:check": "pnpm typecheck",
    "drizzle:pull": "drizzle-kit introspect",
    "drizzle:studio": "drizzle-kit studio",
    "drizzle:check": "drizzle-kit check:sql",
    "db:test": "tsx scripts/test-db.ts",
    "setup:env": "tsx scripts/setup-env.ts",
    "db:gen": "drizzle-kit generate",
    "db:migrate": "tsx scripts/migrate.ts",
    "make:client": "node -e \"const fs=require('fs'),p='src/db';fs.mkdirSync(p,{recursive:true});fs.writeFileSync(p+'/client.ts',`import 'dotenv/config';\\nimport mysql from 'mysql2/promise';\\nimport { drizzle } from 'drizzle-orm/mysql2';\\nimport * as schema from './schema';\\n\\nexport const pool = mysql.createPool({\\n  host: process.env.DB_HOST!,\\n  user: process.env.DB_USER!,\\n  password: process.env.DB_PASSWORD!,\\n  database: process.env.DB_NAME!,\\n  port: Number(process.env.DB_PORT ?? 3306),\\n  connectionLimit: 10,\\n});\\n\\nexport const db = drizzle(pool, { schema, mode: 'default' });\\n`);console.log('âœ“ wrote src/db/client.ts');\"}"
  },
